{
    "name": "litongleo/laravel-getui",
    "description": "一个适用于laravel和lumen框架的个推push推送服务包",
    "type": "library",
    "require": {
        "illuminate/container": "^5.1",
        "php": ">=7.3.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "vlucas/phpdotenv": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ShaoZeMing\\GeTui\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShaoZeMing\\GeTui\\Tests\\": "./tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Shao ZeMing",
            "email": "szm19920426@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ShaoZeMing\\GeTui\\GeTuiServiceProvider"
            ],
            "aliases": {
                "GeTui": "ShaoZeMing\\GeTui\\Facade\\GeTui"
            }
        }
    },
    "minimum-stability":"dev"
}
